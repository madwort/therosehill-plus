{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "therosehill-plus/fancy-header",
    "title": "Fancy Header",
    "category": "text",
    "description": "Adds a header with a hover effect",
    "keywords": [
        "header",
        "underline",
        "hover"
    ],
    "version": "1",
    "textdomain": "therosehill-plus",
    "editorScript": "file:./index.js",
    "attributes": {
        "content": {
            "type": "string",
            "source": "html",
            "selector": "h2"
        },
        "underline_color": {
            "type": "string",
            "default": "#f87171"
        }
    },
    "style": "file:./index.css"
}